<plugin name='fitnesse' version='1.1' grailsVersion='1.3.2 &gt; *'>
  <author>Erik Pragt</author>
  <authorEmail>erik.pragt@jworks.nl</authorEmail>
  <title>Fitnesse Grails Plugin</title>
  <description>The Fitnesse Grails plugin provides a bridge between Fitnesse and Grails.</description>
  <documentation>http://bodiam.github.com/grails-fitnesse-plugin/docs/manual/</documentation>
  <type>FitnesseGrailsPlugin</type>
  <resources>
    <resource>fitnesse.grails.BeginTransactionFixture</resource>
    <resource>fitnesse.grails.CommitFixture</resource>
    <resource>fitnesse.grails.EndTransactionFixture</resource>
    <resource>fitnesse.grails.GivWenZenForGrails</resource>
    <resource>fitnesse.grails.RollbackFixture</resource>
    <resource>nl.jworks.fitnesse.quickstart.EchoFixture</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://givwenzen.googlecode.com/svn/maven2/' url='http://givwenzen.googlecode.com/svn/maven2/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='com.googlecode.givwenzen' name='givwenzen' version='1.0.2' />
      <dependency group='org.htmlparser' name='htmlparser' version='2.1' />
    </compile>
  </dependencies>
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>